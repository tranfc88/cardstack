{{#let (cs-field-editor-for content field) (cs-field-editor-options-for content field) as |editorName editorOptions|}}
  {{#if (is-component editorName)}}
    {{component editorName content=content field=field editorOptions=editorOptions tagName="" enabled=enabled}}
  {{else}}
    {{cs-assert "No such field editor" editorName}}
  {{/if}}
{{/let}}
